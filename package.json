{
  "name": "restbox",
  "version": "1.1.1",
  "description": "REST for CRUD file operations on dropbox",
  "main": "lib/restbox.js",
  "scripts": {
    "test": "tape 'lib/**/*.spec.js'",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc npm test",
    "watch:test": "npm run watcher -- npm test",
    "watch:lint": "npm run watcher -- 'npm run lint'",
    "watcher": "nodemon -w test -w lib --exec",
    "lint": "redrun lint:*",
    "lint:server": "eslint --quiet lib/**/*.js --ignore-pattern **/.spec.js",
    "lint:test": "eslint -c .eslintrc.test lib/**/*.spec.js --no-ignore"
  },
  "dependencies": {
    "@cloudcmd/dropbox": "^4.0.0",
    "apart": "^2.0.1",
    "ashify": "^1.0.2",
    "debug": "^4.1.0",
    "pipe-io": "^3.0.9",
    "ponse": "^3.0.1",
    "pullout": "^3.0.0",
    "readbox": "^2.0.0",
    "router": "^2.0.0-alpha.1",
    "try-to-catch": "^1.1.1",
    "wraptile": "^3.0.0"
  },
  "devDependencies": {
    "@cloudcmd/stub": "^2.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^6.3.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-putout": "^2.0.0",
    "fullstore": "^2.0.2",
    "mock-require": "^3.0.2",
    "nodemon": "^1.11.0",
    "nyc": "^14.1.1",
    "putout": "^5.15.1",
    "redrun": "^7.0.0",
    "request": "^2.79.0",
    "serve-once": "^1.0.0",
    "string-to-stream": "^3.0.1",
    "supertape": "^1.0.3",
    "through2": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/restbox.git"
  },
  "keywords": [
    "rest",
    "crud",
    "files",
    "middleware",
    "dropbox"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/restbox/issues"
  },
  "homepage": "https://github.com/coderaiser/restbox",
  "engines": {
    "node": ">=8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
